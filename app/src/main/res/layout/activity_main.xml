<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <fragment
        android:id="@+id/fragment2"
        android:name="com.example.viewpager_lab4.Fragment.MenuFragment"
        android:layout_width="118dp"
        android:layout_height="409dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toStartOf="@+id/fragment5"
        app:layout_constraintTop_toBottomOf="@+id/fragment4" />


    <fragment
        android:id="@+id/fragment4"
        android:name="com.example.viewpager_lab4.Fragment.DetailFragment"
        android:layout_width="118dp"
        android:layout_height="195dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/fragment2"
        app:layout_constraintEnd_toStartOf="@+id/fragment5" />


    <fragment
        android:id="@+id/fragment5"
        android:name="com.example.viewpager_lab4.Fragment.Image4"
        android:layout_width="294dp"
        android:layout_height="242dp"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/fragment6"
        app:layout_constraintStart_toEndOf="@+id/fragment2" />

    <fragment
        android:id="@+id/fragment6"
        android:name="com.example.viewpager_lab4.Fragment.Image5"
        android:layout_width="297dp"
        android:layout_height="362dp"
        android:layout_marginTop="1dp"
        app:layout_constraintStart_toEndOf="@+id/fragment2"
        app:layout_constraintTop_toBottomOf="@+id/fragment5" />


</androidx.constraintlayout.widget.ConstraintLayout>