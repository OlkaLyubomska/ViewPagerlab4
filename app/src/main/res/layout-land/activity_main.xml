<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/fragment"
        android:name="com.example.viewpager_lab4.Fragment.DetailFragment"
        android:layout_width="262dp"
        android:layout_height="143dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toTopOf="@+id/fragment7"
        app:layout_constraintEnd_toStartOf="@+id/fragment8" />


    <fragment
        android:id="@+id/fragment7"
        android:name="com.example.viewpager_lab4.Fragment.MenuFragment"
        android:layout_width="266dp"
        android:layout_height="201dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toStartOf="@+id/fragment9"
        app:layout_constraintTop_toBottomOf="@+id/fragment" />

    <fragment
        android:id="@+id/fragment8"
        android:name="com.example.viewpager_lab4.Fragment.Image4"
        android:layout_width="415dp"
        android:layout_height="145dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toTopOf="@+id/fragment9"
        app:layout_constraintStart_toEndOf="@+id/fragment" />

    <fragment
        android:id="@+id/fragment9"
        android:name="com.example.viewpager_lab4.Fragment.Image5"
        android:layout_width="414dp"
        android:layout_height="203dp"
        android:layout_marginStart="1dp"
        app:layout_constraintStart_toEndOf="@+id/fragment7"
        app:layout_constraintTop_toBottomOf="@+id/fragment8" />
</androidx.constraintlayout.widget.ConstraintLayout>